{
  "name": "momock",
  "version": "0.1.9",
  "main": "lib/main.js",
  "module": "esm/main.js",
  "bin": "./bin/momock",
  "engines": {
    "node": ">=6.4.0"
  },
  "repository": {
    "url": "https://github.com/liximomo/mo.git",
    "type": "git"
  },
  "author": "liximomo <liximomo@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cheerio": "^1.0.0-rc.2",
    "chokidar": "^1.7.0",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "json-schema-faker": "0.5.0-rc10",
    "puppeteer": "^0.10.0",
    "regenerator-runtime": "^0.11.0",
    "yargs": "^8.0.2"
  },
  "scripts": {
    "build": "npm run build:commonjs && npm run build:esm",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir esm",
    "dev": "rollup -c -w",
    "test": "node test/test.js",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "npm run docs:prepare && gitbook build",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:publish": "npm run docs:clean && npm run docs:build && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:liximomo/mo.git gh-pages --force"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.5",
    "gitbook-cli": "^2.3.2",
    "rimraf": "^2.6.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "no-console": 1
    }
  }
}
